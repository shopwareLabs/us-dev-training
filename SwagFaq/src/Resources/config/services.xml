<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="SwagFaq\Command\DemoDataCommand">
            <argument type="service" id="product.repository"/>
            <argument type="service" id="swag_faq_entry.repository"/>

            <tag name="console.command"/>
        </service>

        <service id="SwagFaq\Subscriber\MySubscriber">
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="SwagFaq\Storefront\Controller\ExampleController" public="true">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="SwagFaq\Core\Content\Example\SalesChannel\ExampleRoute">
            <argument type="service" id="product.repository"/>
        </service>

        <service id="SwagFaq\Core\Content\SwagFaqEntry\SwagFaqEntryDefinition">
            <tag name="shopware.entity.definition" entity="swag_faq_entry" />
        </service>

    </services>
</container>
